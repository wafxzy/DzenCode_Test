{
  "name": "dzen-code-test-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --configuration development --port 4200",
    "start:prod": "ng serve --configuration production --port 4201",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "watch:prod": "ng build --watch --configuration production",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "serve:dist": "npx serve dist/dzen-code-test-front -s -n -l 4202"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.0-next.0",
    "@angular/compiler": "^21.0.0-next.0",
    "@angular/core": "^21.0.0-next.0",
    "@angular/forms": "^21.0.0-next.0",
    "@angular/platform-browser": "^21.0.0-next.0",
    "@angular/router": "^21.0.0-next.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0-next.4",
    "@angular/cli": "^21.0.0-next.4",
    "@angular/compiler-cli": "^21.0.0-next.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.10.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
